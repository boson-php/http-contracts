{
    "name": "boson-php/http-contracts",
    "description": "Provides basic HTTP component structures",
    "type": "library",
    "license": "MIT",
    "homepage": "https://bosonphp.com",
    "support": {
        "issues": "https://github.com/boson-php/boson/issues",
        "source": "https://github.com/boson-php/http-contracts"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Boson\\Contracts\\Http\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Boson\\Contracts\\Http\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "bin-compat": "full",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
